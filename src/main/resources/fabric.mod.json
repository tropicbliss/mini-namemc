  "name": "MiniNameMC",
  "description": "This mod provides info about a specific Minecraft player",
  "authors": [
    "tropicbliss"
  ],
  "contact": {
    "homepage": "https://github.com/tropicbliss/mininamemc",
    "sources": "https://github.com/tropicbliss/mininamemc"
  },

  "license": "MIT",
  "icon": "assets/mininamemc/icon.png",

